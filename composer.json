{
    "name": "palmtree/collection",
    "description": "Maps and Sequences with optional type validation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Palmer",
            "email": "andy@andypalmer.me",
            "homepage": "https://andypalmer.me"
        }
    ],
    "require": {
        "php": "^7.4||^8.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Palmtree\\Collection\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "palmtree/php-cs-fixer-config": "^2.0",
        "vimeo/psalm": "^4.18"
    },
    "autoload-dev": {
        "psr-4": {
            "Palmtree\\Collection\\Test\\": "tests",
            "Palmtree\\Collection\\Test\\Fixture\\": "tests/fixtures"
        }
    }
}
